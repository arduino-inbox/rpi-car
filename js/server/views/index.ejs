<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Car</title>
    <link rel="stylesheet" href="/bootstrap.min.css"/>
    <link rel="stylesheet" href="/bootstrap-theme.min.css"/>
    <link rel="stylesheet" href="/index.css"/>
</head>
<body>
<main class="panel panel-primary">
    <div class="panel-heading">
        <h3 class="panel-title">Runtime: <span class="runtime">0</span></h3>
    </div>
    <div id="dashboard" class="panel-body">
    </div>
    <div id="controls" class="panel-footer">
        <form id="frm">
            <input type="text" id="field" class="text-primary" placeholder="command" readonly="readonly"/>
            <button class="btn btn-danger stop">Stop</button>
            <button class="btn btn-warning bwd">Go backward</button>
            <button class="btn btn-success fwd">Go forward</button>
            <label>Speed:
                <input value="0.05" type="number" min="0" max="1" step="0.05" class="speed"/>
            </label>
            <button class="btn btn-info run">Run</button>
        </form>
    </div>
</main>
<script src="/jquery-2.1.3.min.js"></script>
<script src="/bootstrap.min.js"></script>
<script src="/index.js"></script>
<script src="/socket.io/socket.io.js"></script>
</body>
</html>